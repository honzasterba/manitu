<% if !list.empty? %>
<h2><%= title %></h2>
<% end %>

<% list.each do |site| %>
<div class="site">
	<div class="top">&nbsp;</div>
	<div class="middle"  id="<%= site_content_id(site) %>">
    <%= render :partial => partial, :locals => { :site => site } %>
	</div>
	<div class="bottom">&nbsp;</div>
</div>
<% end %>
